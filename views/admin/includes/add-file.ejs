<div class="col-12 d-none all-forms" id="file">
    <div class="row">
        <div class="col-12" style="margin-bottom: 10px;">
            <form action="/admin/add-file" method="post" enctype="multipart/form-data">
                <h4>add new file</h4>
                <div class="mb-3">
                    <input type="text" name="name" class="form-control">
                </div>
                <div class="mb-3">
                    <input type="file" name="file" class="form-control">
                </div>
                <button type="submit" class="btn btn-dark">submit</button>
            </form>
        </div>
    </div>
</div>